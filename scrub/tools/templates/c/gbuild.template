#!/bin/bash -x
# Change to the build directory
cd ${{GBUILD_BUILD_DIR}}

# Clean the build
${{GBUILD_CLEAN_CMD}}

# Build and capture the output
${{GBUILD_BUILD_CMD}} > ${{TOOL_ANALYSIS_DIR}}/gbuild_build.log 2>&1

# Parse the output into SCRUB format
python3 ${{SCRUB_PATH}}/tools/parsers/get_gbuild_warnings.py ${{TOOL_ANALYSIS_DIR}}/gbuild_build.log ${{RAW_RESULTS_DIR}}/gbuild_raw.scrub